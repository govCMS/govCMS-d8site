{
  "name": "govcms_uikit_starterkit",
  "version": "0.1.0",
  "description": "Front-end build tools for custom Drupal theme.",
  "repository": {
    "type": "git",
    "url": "https://github.com/acquia-pso/cog.git"
  },
  "devDependencies": {
    "breakpoint-sass": "2.7.0",
    "browser-sync": "2.17.x",
    "del": "2.2.x",
    "gulp": "3.9.x",
    "gulp-autoprefixer": "3.1.x",
    "gulp-clean-css": "2.0.13",
    "gulp-eslint": "3.0.x",
    "gulp-load-plugins": "1.3.x",
    "gulp-parker": "0.1.4",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.x",
    "gulp-sass": "2.3.x",
    "gulp-sass-glob": "1.0.6",
    "gulp-sass-lint": "1.2.0",
    "gulp-size": "2.1.x",
    "gulp-sourcemaps": "2.1.x",
    "gulp-stylelint": "^3.9.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
    "kss": "3.0.0-beta.15",
    "node-sass": "3.10.x",
    "node-sass-import-once": "1.2.x",
    "run-sequence": "1.2.x",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "susy": "2.2.12",
    "typey": "1.0.x",
    "@gov.au/accordion": "^0.2.0",
    "@gov.au/animate": "^0.1.3",
    "@gov.au/body": "^0.1.1",
    "@gov.au/breadcrumbs": "^0.2.0",
    "@gov.au/buttons": "^0.4.0",
    "@gov.au/callout": "^0.2.0",
    "@gov.au/control-input": "^0.2.0",
    "@gov.au/core": "^0.1.2",
    "@gov.au/cta-link": "^0.2.0",
    "@gov.au/direction-links": "^0.2.0",
    "@gov.au/footer": "^0.2.0",
    "@gov.au/grid-12": "^0.1.0",
    "@gov.au/header": "^1.1.0",
    "@gov.au/headings": "^0.2.0",
    "@gov.au/inpage-nav": "^0.2.0",
    "@gov.au/keyword-list": "^0.2.0",
    "@gov.au/link-list": "^0.2.0",
    "@gov.au/page-alerts": "^0.2.0",
    "@gov.au/pancake": "^1.1.0",
    "@gov.au/pancake-js": "^1.0.9",
    "@gov.au/pancake-sass": "^1.1.0",
    "@gov.au/progress-indicator": "^0.2.0",
    "@gov.au/responsive-media": "^0.1.0",
    "@gov.au/select": "^0.2.0",
    "@gov.au/skip-link": "^0.2.0",
    "@gov.au/tags": "^0.2.0",
    "@gov.au/text-inputs": "^0.1.1"
  },
  "engines": {
    "node": "6.9.x"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "sass": {
      "modules": false,
      "location": "./sass/",
      "name": "_uikit.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "./js/",
      "name": "uikit.min.js",
      "minifired": true
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 version",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": false,
      "name": false
    }
  },
  "private": true,
  "//": "The postinstall script is needed to work-around this Drupal core bug: https://www.drupal.org/node/2329453",
  "scripts": {
    "bin": "./node_modules/.bin/$1",
    "postinstall": "find node_modules/ -name '*.info' -type f -delete",
    "install-tools": "yarn install --force",
    "uninstall-tools": "rm -r node_modules;",
    "build": "gulp",
    "build:dev": "gulp build:dev",
    "watch": "gulp watch",
    "uikit:refresh": "find css js sass -name '*uikit*' -type f -delete && npm rebuild",
    "postuikit:install": "npm run bin pancake && npm run build:dev"
  }
}
